#include<stdio.h>
void main(){
    int i,n,a[10],mx,mn;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    mx=a[0];
    mn=a[0];
    for(i=0;i<n;i++){
        if(a[i]>mx){
            mx=a[i];
        }
        if(a[i]<mn)
        {
            mn=a[i];
        }
    }
    printf("%d\n",mx);
        printf("%d",mn);
}
